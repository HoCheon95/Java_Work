import java.util.Arrays;

class pra2 {

	public int solution(int num, int n) {
        int answer = 0;
        
        if(num % n == 0) {
        	answer = 1;
        }
        
        return answer;
    }

	public static void main(String[] args) {

		pra2 p = new pra2();

		System.out.println(p.solution(34, 3));

	}

}
