import net.daum.model02.Circ;
import net.daum.model02.Rect;
import net.daum.model02.Tria;

public class AbsEx05 {

	public static void main(String[] args) {
		
		Circ c = new Circ();
		Rect r = new Rect();
		Tria t = new Tria();
		
		c.draw();//오버라이딩 한 메서드 호출
		r.draw();
		t.draw();
		System.out.println("============================");

	}

}
