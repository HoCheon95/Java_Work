import java.util.Arrays;

class pra2 {

	public int solution(int number, int n, int m) {
		int answer = 0;
		
		if(number % n == 0 && number % m == 0) {
			answer = 1;
		}
		
		return answer;
	}

	public static void main(String[] args) {

		pra2 p = new pra2();

		System.out.println(p.solution(55, 10, 5));

	}

}
