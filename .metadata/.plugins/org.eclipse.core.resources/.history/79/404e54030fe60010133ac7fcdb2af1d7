/*
 * 16장.상속.pdf No.28쪽 5번문제)
 */

class TestSuper {

	TestSuper(int i) { }
}

class TestSub extends TestSuper{ 
	// 부모클래스에서 오버로딩된 생성자에 맞게 자식클래스에서 자료형과 전달인자의 개수 반환타입을 동일하게 정의 해야 한다.
	public TestSub(int i) {
		super(i);
	}
	
}

public class Ex16_6 {

	public static void main(String[] args) {
		new TestSub();
	}

}
