import java.util.Arrays;

class pra2 {

	public String solution(String my_string, int k) {
        String answer = "";
        
        for(int i =0; i < k; i++) {
        	answer += my_string;
        }
        
        return answer;
    }

	public static void main(String[] args) {

		pra2 p = new pra2();

		System.out.println(p.solution("love", 10));

	}

}
