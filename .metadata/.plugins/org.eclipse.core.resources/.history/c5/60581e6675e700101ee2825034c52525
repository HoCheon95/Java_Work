/*
 * instanceof 형변환(레퍼런스간의 업캐스팅과 다운캐스팅) 유무 판단 연산자
 */

class HandPhone5 extends Object{
	protected String model;//폰모델명
	protected String number;//폰번호
	
	public HandPhone5() {}//기본 생성자
	
	public HandPhone5(String model, String number) {
		this.model = model;
		this.number = number;
	}// 매개변수 개수가 다른 생성자 오버로딩
	
}

class DicaPhone5 extends HandPhone5{
	protected String pixel; // 화소수
	
	public DicaPhone5() {}
	
	public DicaPhone5()
	
}
public class RefCastEx05 {

	public static void main(String[] args) {

	}

}
