import java.util.Arrays;

class pra2 {

	public int solution(String my_string) {
        int answer = 0;
       
        for(int i =0; i < my_string.length(); i++) {
        	char c = my_string.charAt(i);
        	if( c >= 0 && c <= 9) {
        		answer += c - '0';
        	}
        }
       
        
        return answer;
    }

	public static void main(String[] args) {

		pra2 p = new pra2();

		System.out.println(p.solution("1a2b3c4d123"));

	}

}
