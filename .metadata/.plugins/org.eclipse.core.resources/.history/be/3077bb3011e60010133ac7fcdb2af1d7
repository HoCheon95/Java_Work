 */
class Base{
	Base(){
		System.out.print("Base");
	}// 기본 생성자
}// 부모클래스 Base 

class Alpha extends Base{
	
}// 자손클래스 Alpha
public class Ex16_7 {

	public static void main(String[] args) {
		new Alpha();
		new Base();
		
		/*
		 * 답안)
		 */
	}

}
