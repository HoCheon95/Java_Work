import java.util.Arrays;

class pra2 {

	public int solution(String my_string) {
        int answer = 0;
        int n = 0;
        my_string = my_string.replaceAll("[a-z]", "");
        my_string = my_string.replaceAll("[A-Z]", "");
        n = Integer.parseInt(my_string);
        
        while(n > 0) {
        	answer += n % 10;
        	n /= 10;
        }
        
        return answer;
    }

	public static void main(String[] args) {

		pra2 p = new pra2();

		System.out.println(p.solution("aAb1B2cC34oOp"));

	}

}
